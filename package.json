  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.6",
    "chai": "*",
    "coveralls": "^3.0.2",
    "mocha": "*",
    "nyc": "^12.0.2",
    "source-map-support": "*",
